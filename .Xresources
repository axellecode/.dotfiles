
! urxvt font
URxvt*font: xft:Dejavu Sans Mono:pixelsize=15! \
	    ! xft:Iosevka:style=Regular:size=9,\
	    ! xft:DejaVuSansMono Nerd Font:style=Book,\
	    ! xft:Symbola:style=Rugular,\
	    ! xft:FontAwesome:style=Regular

! URxvt*boldFont:
! urxvt.font: xft:inconsolata:pixelsize=12
! urxvt.boldFont: xft:Liberation Mono:pixelsize=14:style=bold
! urxvt.italicFont: xft:Liberation Mono:pixelsize=14:style=italic
! urxvt.boldItalicFont: xft:Liberation Mono:pixelsize=14:style=bold italic
! setting
URxvt.scrollBar: false

! emacs font
! Emacs.font: DejaVuSansMono-10
Emacs.font: monaco-10

*.FontBackend: xft,x

! color
! special
*.foreground:   #eceff1
*.cursorColor:  #0c86bd
*.background:   #263238

! black
*.color0:       #263238
*.color8:       #37474f

! red
*.color1:       #ff9800
*.color9:       #ffa74d

! green
*.color2:       #8bc34a
*.color10:      #9ccc65

! yellow
*.color3:       #ffc107
*.color11:      #ffa000

! blue
*.color4:       #0c86bd
*.color12:      #81d4fa

! magenta
*.color5:       #e91e63
*.color13:      #ad1457

! cyan
*.color6:       #009688
*.color14:      #26a69a

! white
*.color7:       #cfd8dc
*.color15:      #eceff1

! desactivate iso-mode
! URxvt.iso14755: false
! URxvt.iso14755_52: false

! add copy-past
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
