[general]
status_path = "~/.config/vdirsyncer/status/"


### CONTACT ###
[pair my_contacts_ribstein]
a = "my_contacts_ribstein_local"
b = "my_contacts_ribstein_remote"
collections = ["from a", "from b"]

[storage my_contacts_ribstein_local]
type = "filesystem"
path = "~/.personal/contacts/"
fileext = ".vcf"

[storage my_contacts_ribstein_remote]
type = "carddav"
url = "https://mail.ribstein.xyz/SOGo/dav/sylvain@ribstein.xyz/Contacts/personal/"
username = "sylvain@ribstein.xyz"
password.fetch = ["command","gopass","Social/mail.ribstein.xyz/SOGo"]

